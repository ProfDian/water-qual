# 1. Buat .gitignore yang proper dulu
cat > .gitignore << 'EOF'
# Credentials & Secrets
serviceAccount.json
**/serviceAccount.json
firebase-adminsdk-*.json
.env
.env.*
!.env.example

# Python
__pycache__/
*.py[cod]
*.pyo
*.pyd
venv/
env/
ENV/
*.egg-info/

# IDE
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
Thumbs.db
EOF